// WARNING: Do not forget to rename this file to config.json5, otherwise the bot will not work!
{
  USE_ENV: false, // Make this `true` to store sensitive data in the .env file (or enviroment variables, both work).
  BOT: {
    TOKEN: '', // Bot token, not needed if use_env_for_critical is true.
    INVITE_URL: '', // Invite URL for the bot
    VERSION: '0.0.1' // Bot version
  },
  DATABASE: {
    URI: '', // MongoDB URI, not needed if use_env_for_critical is true.
    NAME: 'ClutterDB', // Database name.
    CACHE_COOLDOWN: 1200 // Cache cooldown in seconds.
  },
  LINKS: {
    ERROR_WEBHOOK: '', // Webhook URL for error reporting, not needed if use_env_for_critical is true.
    GITHUB: 'https://github.com/Clutter-Development/Clutter', // GitHub link
    DISCORD_INVITE: 'https://discord.gg/mVKkMZRPQE', // Bot support server invite link
    DOCUMENTATION: '' // Documentation link
  },
  DEVELOPMENT: {
    SERVER_ID: 944535258722861106, // Server ID for the development server, dev slash commands will be synced here
    DEVELOPMENT_MODE: false // Set this to `true` to enable the development mode (development mode makes it so commands can only be used by bot admins).
  },
  DEFAULTS: {
    PREFIX: ">", // Default prefix for message content commands (not slash!)
    RESPONSES: {
      EMOJIS: {
        ERROR: '',
        SUCCESS: '',
        WARNING: '',
        INFO: ''
      },
      COLORS: {
        ERROR: 0xFFFFFF,
        SUCCESS: 0xFFFFFF,
        WARNING: 0xFFFFFF,
        INFO: 0xFFFFFF
      }
    }
  }
}